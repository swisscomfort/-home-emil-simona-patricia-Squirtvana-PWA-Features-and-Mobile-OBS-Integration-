# Environment configuration - DO NOT COMMIT REAL KEYS!
# Copy this file to .env and fill in your actual API keys

# OpenRouter API (for GPT/Claude)
VITE_OPENROUTER_KEY=your_openrouter_key_here

# ElevenLabs API (for Text-to-Speech)
VITE_ELEVENLABS_KEY=your_elevenlabs_key_here
VITE_ELEVENLABS_VOICE_ID=your_voice_id_here

# Telegram Bot API
VITE_TELEGRAM_API_KEY=your_telegram_token_here

# Backend Configuration
FLASK_API_URL=http://localhost:5000
FLASK_ENV=development
SECRET_KEY=your_secret_key_for_production

# OBS WebSocket (optional)
OBS_HOST=localhost
OBS_PORT=4455
OBS_PASSWORD=

# App Configuration
VITE_APP_NAME=Squirtvana
VITE_APP_VERSION=1.0.0
